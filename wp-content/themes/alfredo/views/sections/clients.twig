<section class="py-5">
    <div class="container">
        <div class="row">
            <div class="col-12">
                <h2>{{clients.title}}</h2>
            </div>
            {% for client in clients.clients %}                
                <div class="col-lg-2 col-12">
                    <img src="{{Image(client.logo).src}}" alt="{{client.logo.alt}}" class="img-responsive">
                </div>
            {% endfor %}
        </div>
    </div>
</section>